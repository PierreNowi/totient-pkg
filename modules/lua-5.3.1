#%Module1.0
proc ModulesHelp { } {
  global dotversion
  puts stderr "Lua 5.3.1"
}
 
module-whatis "Lua 5.3.1"
set basedir $env(HOME)/local/lua-5.3.1

prepend-path PATH $basedir/bin
prepend-path CPATH $basedir/include
prepend-path LD_RUN_PATH $basedir/lib
prepend-path LIBRARY_PATH $basedir/lib
prepend-path MANPATH $basedir/man

setenv LUA_PATH "$basedir/share/lua/5.3/?.lua;$basedir/share/lua/5.3/?/init.lua"
setenv LUA_CPATH "$basedir/lib/lua/5.3/?.so"
