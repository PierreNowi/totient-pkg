#%Module1.0
proc ModulesHelp { } {
  global dotversion
  puts stderr "TAO 2.2.0"
}
 
module-whatis "TAO 2.2.0"
set taodir /share/cs-instructional/cs5220/local/tao-2.2.0

prepend-path CPATH $taodir/include
prepend-path LIBRARY_PATH $taodir/lib
prepend-path LD_LIBRARY_PATH $taodir/lib
setenv TAO_DIR $taodir
