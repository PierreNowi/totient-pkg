#%Module1.0
proc ModulesHelp { } {
  global dotversion
  puts stderr "Intel Cluster Studio XE 2013 SP1"
}

# This was created by sourcing ictvars.sh and doing a diff
# of the resulting environments.  Note that ictvars.sh
# inserts a lot of redundant directories (because it
# sources several sub-scripts).  Note that there are some things
# that probably don't need to be here, like all the Intel MIC
# stuff.

module-whatis "Intel Cluster Studio XE 2013 SP1"

set basedir /share/cs-instructional/cs5220/intel
set composer $basedir/composer_xe_2013_sp1.0.080

prepend-path PATH $basedir/bin
prepend-path CPATH $basedir/include
prepend-path LD_LIBRARY_PATH $basedir/lib
prepend-path LD_LIBRARY_PATH $basedir/lib64
prepend-path LIBRARY_PATH $basedir/lib
prepend-path LIBRARY_PATH $basedir/lib64
prepend-path MANPATH $basedir/man

setenv INTEL_LICENSE_FILE $basedir/licenses:/opt/intel/licenses
setenv MKLROOT $composer/mkl

setenv VTUNE_AMPLIFIER_XE_2013_DIR $basedir/vtune_amplifier_xe_2013
setenv ADVISOR_XE_2013_DIR $basedir/advisor_xe_2013
setenv INSPECTOR_2013_DIR $basedir/inspector_xe_2013

setenv VT_ROOT $basedir/itac/8.1.3.037
setenv VT_LIB_DIR $basedir/itac/8.1.3.037/intel64/itac/lib_impi4
setenv VT_SLIB_DIR $basedir/itac/8.1.3.037/intel64/itac/slib_impi4
setenv VT_ADD_LIBS "-ldwarf -lelf -lvtunwind -lnsl -lm -ldl -lpthread"
setenv VT_MPI impi4
setenv VT_ARCH intel64

setenv IPPROOT $composer/ipp
setenv TBBROOT $composer/tbb
setenv IDB_HOME $basedir/icsxe/2013.1.039/idb/bin/intel64
setenv I_MPI_ROOT $basedir/impi/4.1.1.036

setenv GDBSERVER_MIC $composer/debugger/gdb/target/mic/bin/gdbserver
setenv GDB_CROSS $composer/debugger/gdb/intel64_mic/py26/bin/gdb-mic

prepend-path MANPATH $composer/man/en_US
prepend-path MANPATH $basedir/itac/8.1.3.037/man
prepend-path MANPATH $basedir/impi/4.1.1.036/man

prepend-path LIBRARY_PATH $composer/compiler/lib/intel64
prepend-path LIBRARY_PATH $composer/tbb/lib/intel64/gcc4.4
prepend-path LIBRARY_PATH $composer/mkl/lib/intel64
prepend-path LIBRARY_PATH $composer/ipp/lib/intel64
prepend-path LIBRARY_PATH $composer/ipp/../compiler/lib/intel64

prepend-path MIC_LD_LIBRARY_PATH $composer/compiler/lib/mic
prepend-path MIC_LD_LIBRARY_PATH $composer/tbb/lib/mic
prepend-path MIC_LD_LIBRARY_PATH $composer/mkl/lib/mic
prepend-path MIC_LD_LIBRARY_PATH $composer/mpirt/lib/mic

prepend-path LD_LIBRARY_PATH $composer/tbb/lib/intel64/gcc4.4
prepend-path LD_LIBRARY_PATH $composer/mkl/lib/intel64
prepend-path LD_LIBRARY_PATH $composer/ipp/lib/intel64
prepend-path LD_LIBRARY_PATH $composer/ipp/../compiler/lib/intel64
prepend-path LD_LIBRARY_PATH $composer/mpirt/lib/intel64
prepend-path LD_LIBRARY_PATH $composer/compiler/lib/intel64
prepend-path LD_LIBRARY_PATH $basedir/itac/8.1.3.037/intel64/itac/slib_impi4
prepend-path LD_LIBRARY_PATH $basedir/itac/8.1.3.037/mic/itac/slib_impi4
prepend-path LD_LIBRARY_PATH $basedir/impi/4.1.1.036/intel64/lib

prepend-path MIC_LIBRARY_PATH $composer/tbb/lib/mic

prepend-path CPATH $composer/tbb/include
prepend-path CPATH $composer/mkl/include

prepend-path NLSPATH $composer/debugger/intel64/locale/%l_%t/%N
prepend-path NLSPATH $composer/debugger/gdb/intel64/py26/share/locale/%l_%t/%N
prepend-path NLSPATH $composer/debugger/gdb/intel64_mic/py26/share/locale/%l_%t/%N
prepend-path NLSPATH $composer/mkl/lib/intel64/locale/%l_%t/%N
prepend-path NLSPATH $composer/ipp/lib/intel64/locale/%l_%t/%N
prepend-path NLSPATH $composer/compiler/lib/intel64/locale/%l_%t/%N

prepend-path PATH $composer/debugger/gui/intel64
prepend-path PATH $composer/bin/intel64_mic
prepend-path PATH $composer/bin/intel64
prepend-path PATH $composer/debugger/gdb/intel64/py26/bin
prepend-path PATH $composer/debugger/gdb/intel64_mic/py26/bin
prepend-path PATH $composer/mpirt/bin/intel64
prepend-path PATH $basedir/itac/8.1.3.037/intel64/bin
prepend-path PATH $basedir/impi/4.1.1.036/intel64/bin
prepend-path PATH $basedir/inspector_xe_2013/bin64
prepend-path PATH $basedir/vtune_amplifier_xe_2013/bin64
prepend-path PATH $basedir/advisor_xe_2013/bin64
prepend-path PATH $basedir/icsxe/2013.1.039/bin

prepend-path CLASSPATH $basedir/itac/8.1.3.037/intel64/itac/lib_impi4

prepend-path INCLUDE $composer/mkl/include

