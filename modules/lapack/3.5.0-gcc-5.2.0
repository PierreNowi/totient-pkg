#%Module1.0
proc ModulesHelp { } {
  global dotversion
  puts stderr "LAPACK 3.5.0 (GCC 5.2.0 build)"
}
 
module-whatis "LAPACK 3.5.0 (GCC 5.2.0 build)"
set basedir /share/apps/lapack-3.5.0-gcc-5.2.0

prepend-path CPATH $basedir/include
prepend-path LIBRARY_PATH $basedir/lib
prepend-path LD_RUN_PATH $basedir/lib
