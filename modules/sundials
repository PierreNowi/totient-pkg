#%Module1.0
proc ModulesHelp { } {
  global dotversion
  puts stderr "SUNDIALS 2.5.0"
}
 
module-whatis "SUNDIALS 2.5.0"
set sundialsdir /share/cs-instructional/cs5220/local/sundials-2.5.0

prepend-path CPATH $sundialsdir/include
prepend-path LIBRARY_PATH $sundialsdir/lib
prepend-path LD_LIBRARY_PATH $sundialsdir/lib
