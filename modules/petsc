#%Module1.0
proc ModulesHelp { } {
  global dotversion
  puts stderr "PETSc 3.4.3"
}
 
module-whatis "PETSc 3.4.3"
set petscdir /share/cs-instructional/cs5220/local/petsc-3.4.3

prepend-path CPATH $petscdir/include
prepend-path LIBRARY_PATH $petscdir/lib
prepend-path LD_LIBRARY_PATH $petscdir/lib
setenv PETSC_DIR $petscdir
