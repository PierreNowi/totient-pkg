#%Module1.0
proc ModulesHelp { } {
  global dotversion
  puts stderr "deal.IIi 8.0 (serial build)"
}
 
module-whatis "deal.II 8.0 (serial build)"
set dealii /share/cs-instructional/cs5220/local/deal.II-8.0-serial/

prepend-path CPATH $dealii/include
prepend-path LIBRARY_PATH $dealii/lib
prepend-path LD_LIBRARY_PATH $dealii/lib
prepend-path CMAKE_MODULE_PATH $dealii/cmake
